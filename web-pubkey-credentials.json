{
    "@context": [
      "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^7.0.0/components/context.jsonld",
      "https://linkedsoftwaredependencies.org/bundles/npm/webid-pubkey-credentials-module/^1.0.0/components/context.jsonld"
    ],
    "import": [
      "webid-pubkey-credentials:config/webid-pubkey-credentials.json",
      "webid-pubkey-credentials:config/digest.json",
      "webid-pubkey-credentials:config/signature.json"
    ],
    "@graph": [
      {
        "@type":"Override",
        "overrideInstance": { "@id": "urn:solid-server:default:CredentialsExtractor" } ,
        "overrideParameters": {
          "@type": "CachedHandler",
          "source": {
            "comment": "Supports DPoP and Bearer access tokens, or no credentials.",
            "@type": "UnionCredentialsExtractor",
            "extractors": [
              {
                "@type": "WaterfallHandler",
                "handlers": [
                  { 
                    "@type": "WebidPubkeyCredentialsExtractor",
                    "urlMatch": ["/inbox/"]
                  },
                  {
                    "@type": "DPoPWebIdExtractor",
                    "originalUrlExtractor": {
                      "@type": "OriginalUrlExtractor",
                      "args_identifierStrategy": { "@id": "urn:solid-server:default:IdentifierStrategy" }
                    }
                  },
                  { "@type": "BearerWebIdExtractor" }
                ]
              },
              { "@type": "PublicCredentialsExtractor" }
            ]
          }
        }
      },
      {
        "@type":"Override",
        "overrideInstance": { "@id": "urn:solid-server:default:MetadataParser" } ,
        "overrideParameters": {
          "@type": "ParallelHandler",
          "handlers": [
            { "@id": "urn:solid-server:default:AuthorizationParser" },
            { "@id": "urn:solid-server:default:CookieParser" },
            { "@id": "urn:solid-server:default:ContentTypeParser" },
            { "@id": "urn:solid-server:default:LinkRelParser" },
            { "@id": "urn:solid-server:default:PlainJsonLdFilter" },
            { "@id": "urn:solid-server:default:SlugParser" },
            { "@id": "urn:solid-server:default:DigestParser" },
            { "@id": "urn:solid-server:default:SignatureParser" }
          ]
        }
      },
      {
        "@type":"Override",
        "overrideInstance": { "@id": "urn:solid-server:default:FileDataAccessor" } ,
        "overrideParameters": {
          "@type": "DigestFileDataAccessor",
          "resourceMapper": { "@id": "urn:solid-server:default:FileIdentifierMapper" }
        } 
      }
    ]
  }
  